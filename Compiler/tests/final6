// Queue - push, pop
int main(){
	
	int q[100];
	int qs = 0;
	int qst = 0;

	string msg = "\nOperations:\n1. Push back to the Queue\n2. Pop from the front of the Queue\n3. End\n\n";

	while(1){

		progout("Queue: ");
		int i;
		for(i=qst; i<qs; i=i+1){
			progout(q[i]);
			progout(" " );
		}

		progout(msg);

		int op;
		progin(op);

		switch(op){
			case(1):
				progout("Enter the number: ");
				int t;
				progin(t);
				q[qs] = t;
				qs = qs+1;
                break;
			case(2):
				if(qst < qs){
					qst = qst + 1;
				}
                break;
			case(3):
				return 0;
		}
	}
	return 0;
}
